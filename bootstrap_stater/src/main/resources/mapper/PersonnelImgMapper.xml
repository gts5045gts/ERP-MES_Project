<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bootstrap.study.personnel.mapper.PersonnelImgMapper">

<select id="findByempId" resultType="com.bootstrap.study.personnel.dto.PersonnelImgDTO">
	select *
	from employee_img
	where emp_id = #{empId}


</select>

	
</mapper>